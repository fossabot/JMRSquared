{
  "name": "application-name",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_module,./.babelrc,./package.json,./npm-debug.log --copy-files",
